        "tailwindcss": "^3.4.14",
        "terser": "^5.28.1",
        "typescript": "^5.2.2",
        "vite": "^5.3.1",
        "vite-tsconfig-paths": "^5.1.2"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "biome lint --write",
            "biome format --write"
        ]
    }
